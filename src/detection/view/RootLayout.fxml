<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="750" maxWidth="1000.0" minHeight="750" minWidth="1400.0" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="detection.view.RootController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Файл">
            <items>
              <MenuItem fx:id="saveButton" mnemonicParsing="false" text="Экспорт тем в файл" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Эмоции">
               <items>
                  <MenuItem fx:id="emoButton" mnemonicParsing="false" text="Определить эмоцию текста" />
               </items></Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
